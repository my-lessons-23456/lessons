{
  "compilerOptions": {
    "plugins": [],
    // Moved into next-env.d.ts
    // "types": ["jest", "cypress"],
    // "target": "esnext",
    // "module": "esnext",
    // "jsx": "preserve",
    "lib": ["dom", "es2017"],
    "baseUrl": ".",
    "moduleResolution": "node",
    "strict": true,
    "allowJs": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "isolatedModules": false,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": false,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "incremental": true
  },
  "exclude": [
    "dist",
  ],
  "include": ["**/*.ts", "**/*.tsx"]
}
